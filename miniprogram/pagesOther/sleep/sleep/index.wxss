/**index.wxss**/
Page{
  height: 100%
}
.parentGroup{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 100rpx;
}

.userinfo-avatar {
  width: 256rpx;
  height: 256rpx;
  margin: 20rpx;
  border-radius: 50%;
}
.play-btn {
  width: 100%;
  height: 85rpx;
  display: flex;
  align-items: center;

}
.play-btn-img {
  width: 85rpx;
  height: 85rpx;
  margin: auto;
}
.play-btn-like-img {
  width: 65rpx;
  height: 65rpx;
  margin: auto;
}
.play-btn-like-img:active {
  width: 70rpx;
  height: 70rpx;
  margin: auto;
}

.play-btn-pre-img {
  width: 65rpx;
  height: 65rpx;
  margin: auto;
}
.play-btn-pre-img:active {
  width: 70rpx;
  height: 70rpx;
  margin: auto;
}
.play-btn-next-img {
  width: 65rpx;
  height: 65rpx;
  margin: auto;

}

.play-btn-next-img:active {
   width: 70rpx;
  height: 70rpx;
  margin: auto;
}

.play-btn-list-img {
  width: 65rpx;
  height: 65rpx;
  margin: auto;
}
.play-btn-list-img:active {
  width: 70rpx;
  height: 70rpx;
  margin: auto;
}

.bottomAudio{
  position: fixed;
  display: flex;
  bottom: 0;
  width: 100%;
  height: 65px;
 
  justify-content:space-between;
  align-items: center;
  padding-bottom: 16px;
  
}

.indexBg{
  width: 100%;
  height: 100%;
}

.musicLable{
  color: white;
  font-size: 24px;
}
.musicLableTip{
  color: #eeeeee;
  font-size: 14px;
  margin: 8px;
}
.cloudGroup{
  width: 100%;
  height: 180px;
}
.cloud{
  position: fixed;
  width: 100%;
  height:150px;
}
.cloudTwo{
  position: fixed;
  margin-top: 30px;
  width: 100%;
  height: 150px;
  
}
.cloudCanvas{
  width: 100%;
  height: 150px;
}



.animationAlert {
  width: 100%;
  height: 300px;
   overflow: scroll; 
  position: fixed;
  bottom: -300px; 
  z-index: 2000;
  background: white;
}

.listItem{
  width: 100%;
  height: 50px;
  display: flex;
  flex-direction: row;
  align-items: center;
  padding-left: 16px;
  padding-top: 8px;
  border-bottom: 1px solid #eeeeee
}

.listItem-Img{
  width: 20px;
  height: 20px;
}
.listItem-Name{
  font-size: 14px;
  color: #666666;
  margin-left: 16px;
}
.listItem-Name-Playing{
  font-size: 14px;
  color: red;
  margin-left: 16px;
}
.listItem-Des{
  font-size: 12px;
  color: #8a8a8a;
  margin-left: 16px;
}
.listItem-Des-Playing{
  font-size: 12px;
  color: red;
  margin-left: 16px;
}
